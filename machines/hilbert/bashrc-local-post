alias fsrm='fileutil rm'
alias fsrmr='fileutil rm -R'
alias fstouch='fileutil touch'

alias ginfo='glog.sh INFO'
alias gerror='glog.sh ERROR'
alias gwarning='glog.sh WARNING'

