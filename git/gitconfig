[color]
    diff = auto
    status = auto
    branch = auto
    grep = auto
[user]
    email = ruggeri@self-loop.com
    name = Ned Ruggeri
[branch]
    autosetuprebase = always
[rerere]
    enabled = 1
[alias]
    check = diff --cached
    co = checkout
    fix = commit --am
    llog = log --graph --decorate --name-status
    logp = log --graph --decorate -p --word-diff
    rework = rebase --interactive HEAD~10
    wcheck = diff --cached --color-words
    wdiff = diff --color-words
    wshow = show --color-words
[push]
    default = upstream
[core]
    excludesfile = ~/.gitignore
